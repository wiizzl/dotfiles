[Preprocesses]
disable_ui_logging = 1
remove_rtl_rule = 1
expose_apis = 1
disable_sentry = 1

[AdditionalOptions]
extensions = {{- $first := true -}}{{- range $name, $_ := .extensions.spicetify }}{{- if not $first }}|{{ end }}{{ $name }}.js{{- $first = false }}{{- end }}
custom_apps =
sidebar_config = 0
home_config = 1
experimental_features = 1

[Patch]

[Setting]
{{- if eq .osid "linux-arch" }}
spotify_path = {{ .chezmoi.homeDir }}/.local/share/spotify-launcher/install/usr/share/spotify
{{- end }}
{{- if eq .osid "darwin" }}
spotify_path = /Applications/Spotify.app/Contents/Resources
{{- end }}
prefs_path =
inject_css = 1
check_spicetify_update = 1
current_theme = custom_theme
color_scheme = mocha
inject_theme_js = 1
replace_colors = 1
overwrite_assets = 1
spotify_launch_flags =
always_enable_devtools = 0

[Backup]
version =
with =